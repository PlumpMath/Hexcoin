<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="Host1" value="http://localhost:9003/HexChainService/"/>
    <add key="Peer1" value="http://localhost:9004/HexChainService/"/>
    <add key="Peer2" value="http://localhost:9005/HexChainService/"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
    <httpRuntime/>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  <system.webServer>
    <modules runAllManagedModulesForAllREquests="true"/>
    <directoryBrowse enabled="true"/>
  </system.webServer>
  
  <system.serviceModel>
    <services>
      <service name="JinxService" behaviorConfiguration="myJinxServiceBehave">
        <host>
          <baseAddresses>
           <!-- <add baseAddress="http://localhost:9001/JinxService"/>
            <add baseAddress="net.tcp://localhost:9002/JinxService"/> -->
          </baseAddresses>
        </host>
       <!-- <endpoint address="http://localhost:9001/JinxService" binding="basicHttpBinding" contract="IJinxService"/>
        <endpoint address="net.tcp://localhost:9002/JinxService" binding="netTcpBinding" contract="IJinxService"/> 
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
        <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange"/>-->
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="myJinxServiceBehave">
         <!-- <serviceMetadata httpGetEnabled="true"/>-->
        </behavior>
      </serviceBehaviors>    
    </behaviors>
   </system.serviceModel>
</configuration>
